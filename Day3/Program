import java.util.Scanner;
public class Solution {
    public static void main(String[] args) throws IOException {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int i=1;
        while(i!=11)
        {
            int ans=n*i;
        System.out.println(n+" x "+i+" = "+ans);
        i++;
        }
    }
}
